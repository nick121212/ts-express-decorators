{
  "name": "ts-express-decorators",
  "version": "1.4.7",
  "description": "Use TypeScript decorators to create your Express application and Rest API",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run tsc:compile",
    "tslint": "tslint ./src/*.ts",
    "test": "NODE_ENV=test mocha $NODE_DEBUG_OPTION",
    "test:cov": "NODE_ENV=test nyc npm test",
    "test:travis": "NODE_ENV=test nyc --reporter=text-lcov npm test | coveralls",
    "tsc": "tsc",
    "tsc:compile": "rm -rf ./lib && tsc --project tsconfig.compile.json",
    "version": "npm run build:doc && git add docs",
    "build:doc": "typedoc --target ES6 --module commonjs --name TsHttpExceptions --exclude '**/*.spec.ts' --out docs/ ./src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/romakita/ts-express-decorators.git"
  },
  "keywords": [
    "Express",
    "TypeScript",
    "Decorator",
    "decorators",
    "decorator",
    "typescript",
    "express",
    "Controller",
    "Inject",
    "ioc",
    "ES2015",
    "ES6"
  ],
  "author": "Romain Lenzotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romakita/ts-express-decorators/issues"
  },
  "homepage": "https://github.com/romakita/ts-express-decorators#readme",
  "dependencies": {
    "colors": "^1.1.2",
    "glob": "^7.0.3",
    "reflect-metadata": "^0.1.8",
    "source-map-support": "^0.4.0",
    "ts-httpexceptions": "^2.1.1",
    "ts-log-debug": "^2.2.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/mocha": "^2.2.31",
    "@types/node": "^7.0.0",
    "@types/superagent": "^2.0.34",
    "@types/supertest": "^2.0.0",
    "body-parser": "^1.15.1",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "ejs": "^2.5.5",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "method-override": "^2.3.5",
    "morgan": "^1.7.0",
    "mocha": "^3.3.0",
    "chai": "^3.5.0",
    "multer": "^1.3.0",
    "supertest": "^3.0.0",
    "coveralls": "^2.13.1",
    "nyc": "^10.3.0",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typedoc": "0.5.10",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typescript": "^2.2.2"
  },
  "directories": {
    "lib": "lib",
    "src": "src",
    "test": "test"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "docs",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary"
    ],
    "all": true
  }
}
